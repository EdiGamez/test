@Data
public class CsvBeanBase {

    @JsonProperty("Asset Class")
    private String assetClass;

    @JsonProperty("Factor Type")
    private String factorType;

    @JsonProperty("Subtype")
    private String subType;

    @JsonProperty("Name")
    private String name;

    @JsonProperty("Unit")
    private String unit;

    @JsonProperty("ADO")
    private String ado;

    @JsonProperty("Return Model")
    private String returnModel;

    @JsonProperty("Reference Name")
    private String refName;

    @JsonProperty("Currency")
    private String currency;

    @JsonProperty("Market")
    private String market;

    @JsonProperty("Template")
    private String template;

    @JsonProperty("Nro Days")
    private String nroDays;

    @JsonProperty("Skew")
    private String skew;

    @JsonProperty("Proxy Methodology")
    private String proxyMethodology;

    @JsonProperty("Proxy Status")
    private String proxyStatus;

    @JsonProperty("Start Date")
    private String startDate;

    @JsonProperty("End Date")
    private String endDate;

    @JsonProperty("Alpha")
    private String alpha;

    @JsonProperty("Beta")
    private String beta;
}
@Data
public class CsvBeanProxy {

    @JsonProperty("Proxy 1")
    private String proxy1;

    @JsonProperty("Proxy ADO 1")
    private String proxyAdo1;

    @JsonProperty("Proxy Nro Days 1")
    private String proxyNrDays1;

    @JsonProperty("Proxy Label 1")
    private String proxyLabel1;

    @JsonProperty("Proxy Maturity 1")
    private String proxyMaturity1;

    @JsonProperty("Proxy Swap Maturity 1")
    private String proxySwapMaturity1;

    @JsonProperty("Proxy Option Maturity 1")
    private String proxyOptionMaturity1;

    @JsonProperty("Proxy Skew 1")
    private String proxySkew1;

    @JsonProperty("Proxy 2")
    private String proxy2;

    @JsonProperty("Proxy ADO 2")
    private String proxyAdo2;

    @JsonProperty("Proxy Nro Days 2")
    private String proxyNrDays2;

    @JsonProperty("Proxy Label 2")
    private String proxyLabel2;

    @JsonProperty("Proxy Maturity 2")
    private String proxyMaturity2;

    @JsonProperty("Proxy Swap Maturity 2")
    private String proxySwapMaturity2;

    @JsonProperty("Proxy Option Maturity 2")
    private String proxyOptionMaturity2;

    @JsonProperty("Proxy Skew 2")
    private String proxySkew2;
}
@Data
public class CsvBeanProxy {

    @JsonProperty("Proxy 1")
    private String proxy1;

    @JsonProperty("Proxy ADO 1")
    private String proxyAdo1;

    @JsonProperty("Proxy Nro Days 1")
    private String proxyNrDays1;

    @JsonProperty("Proxy Label 1")
    private String proxyLabel1;

    @JsonProperty("Proxy Maturity 1")
    private String proxyMaturity1;

    @JsonProperty("Proxy Swap Maturity 1")
    private String proxySwapMaturity1;

    @JsonProperty("Proxy Option Maturity 1")
    private String proxyOptionMaturity1;

    @JsonProperty("Proxy Skew 1")
    private String proxySkew1;

    @JsonProperty("Proxy 2")
    private String proxy2;

    @JsonProperty("Proxy ADO 2")
    private String proxyAdo2;

    @JsonProperty("Proxy Nro Days 2")
    private String proxyNrDays2;

    @JsonProperty("Proxy Label 2")
    private String proxyLabel2;

    @JsonProperty("Proxy Maturity 2")
    private String proxyMaturity2;

    @JsonProperty("Proxy Swap Maturity 2")
    private String proxySwapMaturity2;

    @JsonProperty("Proxy Option Maturity 2")
    private String proxyOptionMaturity2;

    @JsonProperty("Proxy Skew 2")
    private String proxySkew2;
}
@Data
@EqualsAndHashCode(callSuper = true)
public class CsvBeanStressTestHipoteticReport extends CsvBeanBase {

    @JsonProperty("Tenor")
    private String tenor; // en vez de label

    // Extiende los campos de proxy
    private CsvBeanProxy proxyData;
}
@Data
@EqualsAndHashCode(callSuper = true)
public class CsvBeanStressTestHistoricReport extends CsvBeanBase {

    @JsonProperty("Label")
    private String label;

    @JsonProperty("Swap Maturity")
    private String swapMaturity;

    @JsonProperty("Option Maturity")
    private String optionMaturity;

    @JsonProperty("interpolationFormula")
    private String interpolationFormula;

    @JsonProperty("valueToInterpolate")
    private String valueToInterpolate;

    @JsonProperty("firstInterpolation")
    private String firstInterpolation;

    @JsonProperty("lastInterpolation")
    private String lastInterpolation;

    @JsonProperty("calibrationMethod")
    private String calibrationMethod;

    @JsonProperty("rateConvention")
    private String rateConvention;

    @JsonProperty("spreadCurve")
    private String spreadCurve;

    @JsonProperty("businessDays")
    private String businessDays;

    private CsvBeanProxy proxyData;
}
@Data
@EqualsAndHashCode(callSuper = true)
public class CsvBeanTaylorReport extends CsvBeanBase {
    private CsvBeanProxy proxyData;
}
