@Test
void testIsProxyValid() {
    CsvBeanTaylorReport bean = new CsvBeanTaylorReport();

    bean.setProxyOne(null);
    assertFalse(bean.isProxyValid()); // proxyOne == null (false)

    bean.setProxyOne("   ");
    assertFalse(bean.isProxyValid()); // proxyOne != null pero est√° blank (false)

    bean.setProxyOne("valid");
    assertTrue(bean.isProxyValid()); // true
}
